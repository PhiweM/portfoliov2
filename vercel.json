{
    "version": 2,
    "builds": [
      {
        "src": "backend/server.js",  
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/projects",
        "dest": "backend/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "backend/index.js"
      }
    ]
  }